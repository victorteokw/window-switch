{
	"name": "window-switch",
	"displayName": "Window Switch",
	"description": "Switch opened files quickly.",
	"version": "0.0.1",
	"publisher": "yeannylam",
	"engines": {
		"vscode": "^1.34.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zhangkaiyulw/window-switch"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "windowSwitch.switchToLastOpenedFile",
				"title": "Switch to Last Opened File",
				"category": "Window Switch"
			},
			{
				"command": "windowSwitch.listOpenedFiles",
				"title": "List Opened Files",
				"category": "Window Switch"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
